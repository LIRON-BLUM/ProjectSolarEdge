@page "/Games"
@*@using ProjectSolarEdge.Shared.Entities
@inject HttpClient httpClient
@using System.Net.Http.Json*@





<h3>Games</h3>


<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Game Name</th>
            <th scope="col">Game Description</th>
            <th scope="col">Creation Date</th>
            @*<th scope="col">Edit</th>*@
        </tr>
    </thead>
    <tbody>
        @foreach (var game in GamesData)
        {
            <tr>
                <th scope="row">@game.ID</th>
                <td>@game.GameName</td>
                <td>@game.GameDescription</td>
                <td>@game.CreationDate</td>
                @*<td><a href="/questions/editQuestion/@game.ID"><button type="button" class="btn btn-primary btn-sm">Edit</button></a></td>*@
            </tr>
        }


    </tbody>
</table>



